{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-var": "warn",
        "semi": "warn",
        "indent": "warn",
        "no-multi-spaces": "warn",
        "space-in-parens": "warn",
        "no-multiple-empty-lines": "warn",
        "prefer-const": "warn",
        "no-use-before-define": "warn",
        "react/prop-types": "warn"
    }
}
